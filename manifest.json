{
  "name": "Inframe",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Cross-site image uploader to user albums for vk.com social network",
  "default_locale" : "en",
  "background": {
    "scripts": [      
      "app/scripts/background.js"
    ]
  },
  "icons": {
      "128": "app/icons/imageinfo-128.png",
      "16": "app/icons/imageinfo-16.png",
      "48": "app/icons/imageinfo-48.png"
  },
  "homepage_url": "http://crea7or.spb.ru",
  "permissions": [ "contextMenus", "tabs", "storage" , "http://*/*", "https://*/*" ]
}
